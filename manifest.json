{
  "name" : "Fresh Searcher",
  "description" : "Adds Insta-Search to the Extension Page on the Wiki",
  "version" : "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png",
      "popup": "popup.html"
  },
  "content_scripts" : [
    {
      "matches": ["https://megatron.2ndsiteinc.com/wiki/pmwiki.php?n=HR.PhoneExtensions"],
      "js" : ["jquery.js", "contentscript.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ]
}